<?xml version="1.0" encoding="utf-8" ?>
<Shell xmlns="http://xamarin.com/schemas/2014/forms"
             xmlns:x="http://schemas.microsoft.com/winfx/2009/xaml"
             x:Class="SqliteExample.AppShell"
       xmlns:views="clr-namespace:SqliteExample.Views"
       xmlns:resources="clr-namespace:SqliteExample.Resources">
    <TabBar>
        <ShellContent
            Title="{x:Static resources:AppResources.TabNotes}"
            Icon="icon_feed.png"
            ContentTemplate="{DataTemplate views:NotesPage}">
            
        </ShellContent>
        <ShellContent
            Title="{x:Static resources:AppResources.TabElectricity}"
            Icon="icon_feed.png"
            ContentTemplate="{DataTemplate views:ElectricityPage}">

        </ShellContent>
        <ShellContent
            Title="{x:Static resources:AppResources.TabWater}"
            Icon="icon_feed.png"
            ContentTemplate="{DataTemplate views:NotesPage}">

        </ShellContent>
        <ShellContent
            Title="{x:Static resources:AppResources.TabGas}"
            Icon="icon_feed.png"
            ContentTemplate="{DataTemplate views:NotesPage}">

        </ShellContent>

        <ShellContent
            Title="{x:Static resources:AppResources.TabAbout}"
            Icon="icon_about.png"
            ContentTemplate="{DataTemplate views:AboutPage}">
            
        </ShellContent>
    </TabBar>
</Shell>